{
  "name": "ntmlauncher",
  "version": "2.0.3-ungitted",
  "description": "NeTask Launcher",
  "main": "main.js",
  "dependencies": {
    "decompress-zip": "^0.3.3",
    "electron-dl": "^3.0.2",
    "extract-zip": "^2.0.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.2.0",
    "hastebin": "^0.2.1",
    "isomorphic-git": "^1.10.2",
    "minecraft-launcher-core": "^3.16.10",
    "request": "^2.88.2",
    "update-electron-app": "^2.0.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "electron": "^12.2.1",
    "electron-builder": "^22.9.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron --trace-warnings .",
    "pack": "electron-builder --dir",
    "distwin": "electron-builder --win",
    "distmac": "electron-builder --mac"
  },
  "build": {
    "appId": "com.mlntcandy.netasklauncher",
    "productName": "NeTask Launcher",
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.adventure-games"
    },
    "linux": {
      "target": "deb"
    }
  },
  "keywords": [],
  "author": "Mlntcandy <me@mlntcandy.com> (https://mlntcandy.com/)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlntcandy/NTMLauncher.git"
  },
  "bugs": {
    "url": "https://github.com/mlntcandy/NTMLauncher/issues"
  },
  "homepage": "https://github.com/mlntcandy/NTMLauncher#readme"
}
